{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx"],
        "alias": {
          "util": "./util",
          "common-util": "./common-util",
          "components": "./components",
          "images": "./public/images",
          "store": "./store"
        }
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    ["inline-react-svg"],
    ["import", { "libraryName": "antd", "style": true }]
  ]
}
